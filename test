import pandas as pd
import matplotlib.pyplot as plt
import seaborn as sns

# Load your DataFrame (assuming you have it in a CSV file)
df = pd.read_csv("/mnt/data/file-CsxNjVLS65x3FSSTJ29Ly6")

# Drop NaN values in the 'SHAP Kurtosis' column
df = df.dropna(subset=['SHAP Kurtosis'])

# Plot bar chart
plt.figure(figsize=(12, 6))
sns.barplot(x=df['Token'], y=df['SHAP Kurtosis'], color='blue')

# Formatting
plt.xlabel("Token")
plt.ylabel("SHAP Kurtosis")
plt.title("Bar Plot of SHAP Kurtosis for Tokens")
plt.xticks(rotation=90)  # Rotate labels for better readability
plt.show()
