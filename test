import asyncio
from app.embedding import Embed

async def main():
    eb_object = Embed("text-embedding")
    batch = [{"chunked_content": "test1234"}]  # adjust to whatever your method expects

    # If embedding_call is async and accepts the batch:
    result = await eb_object.embedding_call(batch)
    print("successfully", result)

if __name__ == "__main__":
    asyncio.run(main())
