from tqdm import tqdm

for data_point in tqdm(chunk_data, total=len(chunk_data), desc="inserting data"):
    metadata = data_point.get("metadata", {})

    meta_data = {
        "parent_node": [metadata.get("parent_node")] if metadata.get("parent_node") is not None else ["NA"],
        "prev_node": [metadata.get("prev_node")] if metadata.get("prev_node") is not None else ["NA"],
        "child_nodes": metadata.get("child_nodes") if metadata.get("child_nodes") is not None else ["NA"],
        "next_node": [metadata.get("next_node")] if metadata.get("next_node") is not None else ["NA"],
    }
