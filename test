plt.figure(figsize=(10, 6))

sns.kdeplot(reference, label='Reference', fill=True, alpha=0.5, color='blue')
sns.kdeplot(current, label='Current', fill=True, alpha=0.5, color='orange')

# Add JS Divergence annotation
plt.title(f"Reference vs Current Distribution\nJS Divergence: {js_divergence:.4f}", fontsize=14)
plt.xlabel("Feature Space", fontsize=12)
plt.ylabel("Density", fontsize=12)
plt.legend()
plt.tight_layout()
plt.show()
