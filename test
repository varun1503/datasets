def get_vector_table_metadata(session, use_case_id: int, embedding_size: int) -> list:
    """
    Fetch metadata rows from gn_vctr.knw_str based on use case ID and minimum embedding size.

    Args:
        session: Cassandra DB session object.
        use_case_id (int): Use case ID to filter.
        embedding_size (int): Minimum embedding dimension.

    Returns:
        list: List of row objects containing table metadata.
    """
    query = f"""
    SELECT *
    FROM gn_vctr.knw_str
    WHERE us_cs_id = {use_case_id}
      AND embd_mdl_dm_n >= {embedding_size}
    ALLOW FILTERING;
    """
    try:
        result = session.execute(query)
        return list(result)  # Convert to list for easier reuse
    except Exception as e:
        print(f"‚ùå Failed to fetch metadata from gn_vctr.knw_str: {e}")
        return []
