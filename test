# Step 1: Convert original_triples into a string list
original_triples_list = [",".join(j for j in tuple_triple) for tuple_triple in original_triples]

# Step 2: Build a dictionary from retrieved_quadruples using row_id as key
retrieved_quadruples_dict = {i.row_id: i for i in retrieved_quadruples}

# Step 3: Match values from triplate_dict to retrieved_quadruples_dict
a = {
    triplate_dict[i]: retrieved_quadruples_dict[triplate_dict[i]]
    for i in original_triples_list
    if i in triplate_dict and triplate_dict[i] in retrieved_quadruples_dict
}
