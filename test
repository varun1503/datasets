import re

texts = [
    "* Average Gini across the 8 out-of-time periods (refer table 5.5), Table 35.8",
    "Table 35.8 Average Gini across the 8 out-of-time periods"
]

for i in texts:
    text = i.strip()

    if (
        re.match(r"^(Table|Figure)\s+\d+(\.\d+)?[:.\s]", text, re.IGNORECASE)
        and len(text) < 300
    ):
        print("True")
    else:
        print("False")
