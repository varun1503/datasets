docx_path = "your_document.docx"
pages = extract_docx_with_structure(docx_path)

for i, page in enumerate(pages, 1):
    print(f"\n--- Page {i} ---")
    print("Paragraphs:")
    for p in page['paragraphs']:
        print("-", p)

    print("Tables:")
    for t in page['tables']:
        for row in t:
            print(row)

    print("Images:")
    for img in page['images']:
        print("Label:", img)
