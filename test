match = re.match(r'^((\d+\.)+\d*)', text)
if match:
    section_number = match.group(1).rstrip('.')
else:
    section_number = self.get_section_number(level)

clean_text = re.sub(r'^((\d+\.)+\d*\s*)', '', text).strip()

heading_item = {
    'id': heading_id,
    'type': 'heading',
    'text': f"{section_number} {clean_text}",
    'section_number': section_number,
    'level': level,
    'source': self.file_name,
    'child_ids': []
}
