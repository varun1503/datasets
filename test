from langchain.schema import Document

final_chunk = []

for i in range(len(chunks_data)):
    print(i)
    
    # Make sure 'metadata' exists
    if 'metadata' not in chunks_data[i]:
        chunks_data[i]['metadata'] = {}

    # Move 'quadruples' into metadata
    chunks_data[i]['metadata']['quadruples'] = chunks_data[i]['quadruples']

    # Create Document and add to final_chunk
    final_chunk.append((
        Document(
            page_content=chunks_data[i]['content'],
            metadata=chunks_data[i]['metadata']
        ), 
        0.1
    ))
